<rm:rmap
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:rm="http://www.eclipse.org/buckminster/RMap-1.0"
	xmlns:mp="http://www.eclipse.org/buckminster/MavenProvider-1.0"
	xmlns:bc="http://www.eclipse.org/buckminster/Common-1.0">

	<rm:site name="default"/>

	<rm:searchPath name="maven">
		<rm:provider xsi:type="mp:MavenProvider" readerType="maven" componentType="maven" mutable="false" source="false">
			<rm:uri format="http://www.ibiblio.org/maven#{0}">
				<bc:property key="buckminster.component" />
			</rm:uri>
			<mp:mappings>
				<mp:entry name="se.tada.util.sax" groupId="se.tada" artifactId="tada-sax"/>
				<mp:entry name="org.apache.log4j" groupId="log4j" artifactId="log4j"/>
				<mp:rule
					fromPattern="org\.apache\.commons\.(.*)" fromReplacement="commons-$1/commons-$1"
					toPattern="[^/]+/commons-(.*)" toReplacement="org.apache.commons.$1" />
			</mp:mappings>
		</rm:provider>
	</rm:searchPath>

	<rm:locator searchPathRef="maven" pattern=".*" />
</rm:rmap>
