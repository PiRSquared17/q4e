<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ecf.filetransfer.urlStreamHandlerService">
      <urlStreamHandlerService
            protocol="maven"
            serviceClass="org.devzuz.q.internal.ecf.provider.filetransfer.MavenURLStreamHandlerService">
      </urlStreamHandlerService>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.ecf.core.BaseContainer"
            class="org.devzuz.q.internal.ecf.provider.filetransfer.MavenRetrieveFileTransferAdapterFactory">
         <adapter type="org.eclipse.ecf.filetransfer.IRetrieveFileTransferContainerAdapter"/>
      </factory>
   </extension>
   <extension
         point="org.eclipse.ecf.identity.namespace">
      <namespace
            class="org.devzuz.q.internal.ecf.provider.filetransfer.MavenRetrieveFileTransferNamespace"
            name="maven.filetransfer.namespace">
      </namespace>
   </extension>

</plugin>
